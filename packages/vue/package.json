{
  "name": "@blro/ui-primitives-vue",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blro0319/ui-primitives.git",
    "directory": "packages/vue"
  },
  "license": "MIT",
  "type": "module",
  "files": [
    "./dist"
  ],
  "main": "./dist/primitives-vue.cjs",
  "module": "./dist/primitives-vue.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/primitives-vue.mjs",
      "require": "./dist/primitives-vue.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "dev": "vite --host --port 3190",
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --project tsconfig.build.json && tsc-alias --project tsconfig.build.json"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@blro/body-scroll-lock": "^1.0.0",
    "@vueuse/core": "^10.2.1",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@babel/types": "^7.22.5",
    "@tsconfig/node18": "^18.2.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.3.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/tsconfig": "^0.4.0",
    "sass": "^1.63.6",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6",
    "v-lazy-show": "^0.2.3",
    "vite": "^4.3.9",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue-tsc": "^1.8.3"
  }
}
